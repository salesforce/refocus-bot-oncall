{
  "name": "Oncall-Bot",
  "displayName": "Oncall",
  "version": "1.2.6",
  "description": "A bot for PagerDuty integration",
  "url": "https://git.soma.salesforce.com/Refocus-Bots/Oncall-Bot",
  "scripts": {
    "start-ui": "webpack-dev-server --open",
    "start": "npm run build-ui && node index.js",
    "build-ui": "webpack --config webpack.config.js",
    "compile": "babel src -d lib",
    "lint": "eslint --ext=js --ext=jsx .",
    "postinstall": "npm run build-ui",
    "coverage": "nyc node_modules/mocha/bin/_mocha tests/*",
    "test": "mocha --recursive tests/* && npm run lint && npm run coverage",
    "test-dev": "nodemon --ext js --watch ./ --exec 'mocha ./test'"
  },
  "dependencies": {
    "@salesforce/refocus-bdk": "^1.10.1",
    "dotenv": "4.0.0",
    "email-validator": "^1.0.7",
    "express": "^4.16.4",
    "handlebars": "^4.7.6",
    "jsforce": "^1.9.2",
    "lodash": "^4.17.15",
    "lodash-es": "^4.17.15",
    "moment": "^2.27.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-html-parser": "^2.0.2",
    "react-select": "^3.1.0",
    "sendgrid": "^5.1.2",
    "serialize-javascript": "^3.0.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "superagent": "^3.6.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": "> 0.25%, not dead"
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-regenerator",
      "@babel/plugin-transform-runtime"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-regenerator": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.9.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "chai": "^4.0.2",
    "css-loader": "^2.1.1",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.6",
    "handlebars-loader": "^1.7.1",
    "html-webpack-plugin": "^4.3.0",
    "lodash-webpack-plugin": "^0.11.5",
    "mocha": "^8.0.1",
    "nyc": "^15.0.1",
    "sinon": "^2.3.7",
    "style-loader": "^0.18.2",
    "terser-webpack-plugin": "^3.0.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "zip-webpack-plugin": "^3.0.0"
  },
  "metadata": {
    "actions": [
      {
        "name": "getServices",
        "parameters": []
      },
      {
        "name": "getIncidentDetails",
        "parameters": [
          {
            "name": "incidents",
            "type": "ARRAY"
          }
        ]
      },
      {
        "name": "pagerServices",
        "parameters": [
          {
            "name": "services",
            "type": "ARRAY"
          },
          {
            "name": "message",
            "type": "STRING"
          }
        ]
      },
      {
        "name": "getRecommendations",
        "parameters": [
          {
            "name": "caseData",
            "type": "STRING"
          }
        ]
      }
    ],
    "data": [],
    "settings": []
  }
}
