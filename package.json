{
  "name": "Oncall-Bot",
  "displayName": "Oncall",
  "version": "1.1.5",
  "description": "A bot for PagerDuty integration",
  "url": "https://git.soma.salesforce.com/Refocus-Bots/Oncall-Bot",
  "scripts": {
    "start-ui": "webpack-dev-server --open",
    "start": "npm run build-ui && node index.js",
    "build-ui": "webpack --config webpack.config.js",
    "lint": "eslint --ext=js --ext=jsx .",
    "postinstall": "npm run build-ui",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha tests/*",
    "test": "mocha --recursive tests/* && npm run lint && npm run coverage",
    "test-dev": "nodemon --ext js --watch ./ --exec 'mocha ./test'"
  },
  "dependencies": {
    "@salesforce/refocus-bdk": "^1.7.1",
    "chai": "^4.0.2",
    "dotenv": "4.0.0",
    "dotenv-webpack": "1.5.4",
    "email-validator": "^1.0.7",
    "express": "^4.16.4",
    "handlebars": "^4.1.2",
    "jsforce": "^1.9.2",
    "lodash": "^4.17.15",
    "mocha": "^6.1.3",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-html-parser": "^1.0.3",
    "react-select": "^1.0.1",
    "sendgrid": "^5.1.2",
    "serialize-javascript": "^2.1.1",
    "sinon": "^2.3.7",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "superagent": "^3.6.0"
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "es2015",
      "stage-2"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^2.1.1",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "handlebars-loader": "^1.7.1",
    "html-webpack-plugin": "^2.28.0",
    "istanbul": "^0.4.5",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "1.2.7",
    "url-loader": "^1.1.2",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^3.3.1",
    "zip-webpack-plugin": "2.0.0"
  },
  "metadata": {
    "actions": [
      {
        "name": "getServices",
        "parameters": []
      },
      {
        "name": "getIncidentDetails",
        "parameters": [
          {
            "name": "incidents",
            "type": "ARRAY"
          }
        ]
      },
      {
        "name": "pagerServices",
        "parameters": [
          {
            "name": "services",
            "type": "ARRAY"
          },
          {
            "name": "message",
            "type": "STRING"
          }
        ]
      }
    ],
    "data": [],
    "settings": []
  }
}
